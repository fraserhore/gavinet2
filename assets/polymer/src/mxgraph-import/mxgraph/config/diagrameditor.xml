<mxEditor defaultGroup="group" defaultEdge="connector">
	<include name="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/config/editor-commons.xml"/>

	<ui>
		<add as="graph" element="graph"/>
		<add as="status" element="status"/>
		<add as="toolbar" element="toolbar"/>
	</ui>
	<Array as="templates">
		<add as="text">
			<Text label="Text" contentType="text" href="">
				<mxCell vertex="1" style="text">
					<mxGeometry as="geometry" width="80" height="20"/>
				</mxCell>
			</Text>
		</add>
		<add as="input">
			<Input label="Input" description="" relatedOutputId="" id="" contentType="input">
				<mxCell vertex="1" style="input">
					<mxGeometry as="geometry" width="80" height="60"/>
				</mxCell>
			</Input>
		</add>
		<add as="activity">
			<Activity label="Activity" description="" duration="" personDays="" policies="" kpis="" supportingTechnology="" status="" id="" contentType="activity" recommendations="" comments="">
				<mxCell vertex="1" style="activity">
					<mxGeometry as="geometry" width="80" height="60"/>
				</mxCell>
			</Activity>
		</add>
		<add as="decision">
			<Decision label="Decision" description="" duration="" kpis="" supportingTechnology="" status="" id="" contentType="decision">
				<mxCell vertex="1" style="decision">
					<mxGeometry as="geometry" width="80" height="60"/>
				</mxCell>
			</Decision>
		</add>
		<add as="output">
			<Output label="Output" description="" duration="" kpis="" supportingTechnology="" status="" id="" contentType="output" mediaType="" storageLocation="">
				<mxCell vertex="1" style="output">
					<mxGeometry as="geometry" width="80" height="60"/>
				</mxCell>
			</Output>
		</add>
		<add as="outcome">
			<Outcome label="Outcome" description="" duration="" kpis="" supportingTechnology="" status="" id="" contentType="outcome">
				<mxCell vertex="1" style="outcome">
					<mxGeometry as="geometry" width="80" height="60"/>
				</mxCell>
			</Outcome>
		</add>
		<add as="impact">
			<Impact label="Impact" description="" duration="" kpis="" supportingTechnology="" status="" id="" contentType="impact">
				<mxCell vertex="1" style="impact">
					<mxGeometry as="geometry" width="80" height="60"/>
				</mxCell>
			</Impact>
		</add>
		<add as="image">
			<Image label="Image" href="">
				<mxCell vertex="1" style="image">
					<mxGeometry as="geometry" width="80" height="50"/>
				</mxCell>
			</Image>
		</add>
		
		<add as="role">
			<Role label="Role" description="" duration="" kpis="" supportingTechnology="" status="" id="" contentType="actor">
				<mxCell vertex="1" style="swimlane" connectable="1">
					<mxGeometry as="geometry" width="900" height="100"/>
				</mxCell>
			</Role>
		</add>
		<add as="hline">
			<Hline label="Hline" href="">
				<mxCell vertex="1" style="ellipse">
					<mxGeometry as="geometry" width="60" height="10"/>
				</mxCell>
			</Hline>
		</add>
		<add as="connector">
			<Connector label="">
				<mxCell edge="1">
					<mxGeometry as="geometry" relative="1"/>
				</mxCell>
			</Connector>
		</add>
		<add as="offPageReference">
			<OffPageReference label="Off Page Reference"  href="" targetId="">
				<mxCell vertex="1" style="offPageReference">
					<mxGeometry as="geometry" width="80" height="40"/>
				</mxCell>
			</OffPageReference>
		</add>
	</Array>
	<mxGraph as="graph" alternateEdgeStyle="verticalConnector" allowLoops="1" dropEnabled="1" >
		<add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
		<add as="isSwimlane"><![CDATA[
			function (cell)
			{
				if(cell !== null)
				{
					if(cell.style === 'swimlane')
						return true;
					else
						return mxUtils.isNode(this.model.getValue(cell), 'swimlane');
				}
				else
					return mxUtils.isNode(this.model.getValue(cell), 'swimlane');
				
			}
		]]></add>
		<add as="getTooltipForCell"><![CDATA[
			function(cell)
			{
				var id = cell.getId();
				
				return '<b>'+cell.getAttribute('label')+
						'</b> ('+cell.getId()+')'+
						'<br>Style: '+cell.getStyle()+
						'<br>Connections: '+cell.getEdgeCount()+
						'<br>Children: '+cell.getChildCount();
			}
		]]></add>
		<add as="convertValueToString"><![CDATA[
			function(cell)
			{
				//return cell.getAttribute('label');
				return cell.value.label;
			}
		]]></add>
		<include name="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/config/stylesheet.xml"/>
		<mxGraphModel as="model">
			<add as="valueForCellChanged"><![CDATA[
				function(cell, value)
				{
					var previous = null;

					if (value == null || isNaN(value.nodeType))
					{
						previous = cell.value.getAttribute('label');

						if (value == null)
						{
							cell.value.removeAttribute('label');
						}
						else
						{
							cell.setAttribute('label', value);
						}
					}
					else
					{
						previous = cell.value;
						cell.value = value;
					}

					return previous;
				}
			]]></add>
			<root>
				<Graph label="Graph" href="http://www.mxgraph.com/" id="0" PMTversion="20150826_v1.1">
					<mxCell/>
				</Graph>
				<Layer label="Default Layer" id="1" spId="" spGuid="">
					<mxCell parent="0"/>
				</Layer>
			</root>
		</mxGraphModel>
	</mxGraph>
	<mxDefaultToolbar as="toolbar">
		<add as="connect" mode="connect" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/connect.png"><![CDATA[
			function (editor)
			{
				if (editor.defaultEdge != null)
				{
					editor.defaultEdge.style = null;
				}
			}
		]]></add>
		<add as="connect" mode="connect" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/straight.png"><![CDATA[
			function (editor)
			{
				if (editor.defaultEdge != null)
				{
					editor.defaultEdge.style = 'straightConnector';
				}
			}
		]]></add>
		<add as="connect" mode="connect" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/arrow.png"><![CDATA[
			function (editor)
			{
				if (editor.defaultEdge != null)
				{
					editor.defaultEdge.style = 'arrowConnector';
				}
			}
		]]></add>
		<add as="Text" template="text" style="text" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/text.png"/>
		<add as="Border" template="text" style="textBorder" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/text_border.png"/>
		<add as="Input" template="input" style="input" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/ic_crop_5_4_black_48dp_1x_input.png"/>
		<add as="Activity" template="activity" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/Activity.png"/>
		<add as="Decision" template="decision" style="decision" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/Decision.png"/>
		<add as="Decision Extended" template="decision" style="decisionExtended" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/hexagon_filled.png"/>
		<add as="Output" template="output" style="output" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/ic_crop_5_4_black_48dp_1x_output.png"/>
		<add as="Outcome" template="outcome" style="outcome" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/ic_crop_5_4_black_48dp_1x_blue.png"/>
		<add as="Impact" template="impact" style="impact" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/ic_crop_5_4_black_48dp_1x_green_2.png"/>
		<add as="Role" template="role" style="swimlane" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/swimlane.png"/>
		<add as="Database" template="database" style="database" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/cylinder.png"/>
		<add as="Line" template="hline" style="hline" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/hline.png"/>
		<add as="Image" template="image" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/ic_image_black_48dp_1x.png"/>
		<add as="Off Page Reference" template="offPageReference" style="offPageReference" icon="/polymer-starter-kit/app/elements/mxgraph-import/mxgraph/images/MaterialIcons/triangle_blue.png"/>
	</mxDefaultToolbar>
</mxEditor>
